{
    "name": "netlogix/nlxfilterdescription",
    "description": "With this plugin it is possible to prefix an active filter value with its specific filter description.",
    "type": "shopware-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "netlogix GmbH & Co. KG",
            "email": "websolutions@netlogix.de"
        }
    ],
    "extra": {
        "installer-name": "nlxFilterDescriptionShopware"
    },
    "autoload": {
        "psr-4": {
            "nlxFilterDescriptionShopware\\": "."
        }
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "shopware/shopware": "^5.2",
        "solutiondrive/sd-test-environment-shopware": "~0.2",
        "solutiondrive/standalone-build-tools": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "standalone-build-tools": {
            "phpspec-standalone.php7.2.phar": "http://build-tools.cloud.solutiondrive.de/phar/phpspec-standalone.php7.2.phar",
            "phpspec-standalone.php7.1.phar": "http://build-tools.cloud.solutiondrive.de/phar/phpspec-standalone.php7.1.phar",
            "ecs-standalone.phar": "http://build-tools.cloud.solutiondrive.de/phar/coding-standard-standalone.ecs.php7.2.phar",
            "easy-coding-standard-php7.1.yml": "http://build-tools.cloud.solutiondrive.de/phar/easy-coding-standard-php7.1.yml"
        }
    }
}
